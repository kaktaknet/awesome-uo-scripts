[FUNCTION shadow_wep]
IF (<SRC.BRAIN>)
RETURN 0
ELSEIF (<SRC.KARMA> > 100)
	SRC.STAM=(<SRC.STAM> +- <eval RAND(20))
	RETURN 0
ELSEIF (<SRC.KILLS> < 1)
	SRC.STAM=(<SRC.STAM> +- <eval RAND(20))
	RETURN 0
ELSE
RETURN 0
ENDIF


