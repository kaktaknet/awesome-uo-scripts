//
// Agapite Weapons
//

[FUNCTION agapite_wep]
IF (<SRC.BRAIN>)
RETURN 0
ELSEIF (<SRC.KARMA> < <SERV.PLAYERNEUTRAL>)
	SRC.DAMAGE=20,50
	SRC.EFFECT=3,i_fx_curse,6,15,1
	SRC.SOUND=snd_SPELL_POISON
	RETURN 0
ELSEIF (<SRC.KILLS> > <SERV.MURDERMINCOUNT>)
	SRC.DAMAGE=20,50
	SRC.EFFECT=3,i_fx_curse,6,15,1
	SRC.SOUND=snd_SPELL_POISON
	RETURN 0
ELSEIF <SRC.FLAGS>&statf_criminal
	SRC.DAMAGE=20,50
	SRC.EFFECT=3,i_fx_curse,6,15,1
	SRC.SOUND=snd_SPELL_POISON
	RETURN 0
ELSE
RETURN 0
ENDIF


