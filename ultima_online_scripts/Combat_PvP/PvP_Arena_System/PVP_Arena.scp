[itemdef i_die_stone] //This stone kills players so they can enter the pvp arena.

id=i_grave_stone

name=Click me and die!



on=@dclick

src.kill



[itemdef i_telepvp_tile]//put this tile in each box where the pvp stone is contained so they can enter the pvp arena. make this a type 17. goes with pic 2 and 3.

id=1174

name=Go to Pvp Arena



on=@step

IF ! <eval <src.tag.equip>>

  SRC.SYSMESSAGE You can't use this twice!

ELSE

Src.tag.equip=01

src.go 5425,1220//change to where ever the actual pvp fighting pit is.

endif



[itemdef i_death_tile]//this is the tile to go to the actual pvp arena, and bring them back from when they die in action. you must be dead to enter, so exploiting is impossible. make sure to set type 17. put this in your telecenter or the city, where ever you want.

id=1174

name=Pvp Arena



on=@step

IF ! ( <SRC.FLAGS> & 02 )

src.say I must be dead to enter!

return 1

else

src.pack_to_bank

src.go 5410,1214//make this the box that contains an ankh, a red tile to go to the red prep box, a blue tile for the blue prep box, and the moon tile to leave the pvp arena.





[FUNCTION pack_to_bank]//moves any newbie items to the bank...

IF (0<FINDLAYER(21).FINDCONT(0).UID>==0)

RETURN 1

ENDIF

FINDLAYER(21).FINDCONT(0).CONT=<FINDLAYER(layer_bankbox).UID>

pack_to_bank

RETURN 1



[itemdef i_color_tile]//now... you set type 20 to the tile, then cast 45 on it to mark it. mark it where the room for color a would be. make a second one for color b box. then set both tiles to type 17 so they can be telepads. goes with pic 1.

id=1174

name=Temp Pvp Arena



on=@step

IF  ( <SRC.FLAGS> & 02 )

src.say I must be Alive to enter!

return 1

else

return 0



[itemdef i_moon_tile]//put this where players rezz, next to both the teams, in case the person wants to leave. goes with pic 1

id=1174

name=To Moonglow



on=@step

src.go Moonglow

