ON=*bring* cove
IF <SRC.RESTEST 1 i_mem_gate_cove>
SAYUA 0481,0,0,DEU, Ihr wiederholt euch !
RETURN 1 
	ELSE 
		IF <SRC.RESTEST 50 i_gold> 
		SRC.CONSUME=50 i_gold 
		SRC.NEWITEM=i_mem_gate_cove
		SRC.ACT.EQUIP
		SRC.ACT.ATTR=ATTR_DECAY
		SRC.ACT.TIMER=10
		SAYUA 0481,0,0,DEU, Vas Rel Por
		ANIM 17
	RETURN 1 
	ELSE 
		SAYUA 0481,0,0,DEU, Du hast nicht genug Gold. 
	ENDIF
ENDIF 

