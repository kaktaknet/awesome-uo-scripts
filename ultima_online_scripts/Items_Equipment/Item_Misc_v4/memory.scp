	IF (<eval <TAG.GAME_MEMORY_BUTTON_OVER>>==0)
		TRY=TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_PRESS>>_FACE=<EVAL <TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_PRESS>>_OVERFACE>>
