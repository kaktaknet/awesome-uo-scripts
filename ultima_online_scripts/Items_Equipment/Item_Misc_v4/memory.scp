	ELIF (<eval <TAG.GAME_MEMORY_BUTTON_OVER>> > 0)
		IF (<EVAL <TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_OVER>>_OVERFACE>>==<EVAL <TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_PRESS>>_OVERFACE>>)
