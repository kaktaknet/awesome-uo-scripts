			TRY=TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_OVER>>_FACE=<EVAL <TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_OVER>>_OVERFACE>>
			TAG.GAME_MEMORY_BUTTON_OVER=0
			TAG.GAME_MEMORY_COMPLETED=<eval <TAG.GAME_MEMORY_COMPLETED>+1>
			IF (<EVAL <TAG.GAME_MEMORY_COMPLETED>>==10)
				MENU m_game_memory_finish
			ELSE
				TAG.GAME_MEMORY_BUTTON_OVER=0
				DIALOG d_memory
			ENDIF
		ELSE
			TRY=TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_PRESS>>_FACE=<TAG.GAME_MEMORY_FACE1>	
			TRY=TAG.GAME_MEMORY_BUTTON<EVAL <TAG.GAME_MEMORY_BUTTON_OVER>>_FACE=<TAG.GAME_MEMORY_FACE1>
			TAG.GAME_MEMORY_BUTTON_OVER=0		
			DIALOG d_memory
		ENDIF	
	ENDIF
