[FUNCTION harmony]
IF (<SRC.FINDID.i_bard_harmony.UID>)
SRC.FINDID.i_bard_harmony.REMOVE
ENDIF
IF (<EVAL <TAG.BARD>>=1)
SRC.SYSMESSAGE You sing a beautiful song of harmony, reacting away from creatures.
SRC.NEWITEM i_bard_harmony
SRC.ACT.EQUIP
RETURN 1
ELSE
SRC.SYSMESSAGE You are not a bard.
RETURN 1

[ITEMDEF i_bard_harmony]
DEFNAME=i_bard_harmony
ID=i_handr_1
TYPE=t_eq_script
NAME=Bard Harmony

ON=@CREATE
	TIMER 1
	ATTR=attr_decay

ON=@EQUIP
	TIMER 1

ON=@TIMER
	IF (<CONT.MANA><=5)
	CONT.SYSMESSAGE The song, harmony stops.
	CONT.FINDID.i_rune_reactive_armor.timer 1
	REMOVE
	RETURN 1
	ELSE
	CONT.SYSMESSAGE You feel in harmony.
	CONT.SPELLEFFECT=s_reactive_armor,750
	CONT.MANA=<CONT.MANA> + (-10)
	TIMER 5
	RETURN 1

[FUNCTION wolfform]
IF (<SRC.FINDID.i_druid_wolfform.UID>)
SRC.SYSMESSAGE You are already in wolfform.
RETURN 1
ENDIF
IF (<EVAL <TAG.DRUID>>=1)
SRC.SYSMESSAGE You transform yourself into a wolf form, regenerating quickly.
SRC.NEWITEM i_druid_wolfform
SRC.ACT.TAG.PREVCHAR <BODY>
SRC.ACT.EQUIP
BODY c_wolf
FIX
SAYua 0,0,1,eng, [ enters into wolf form ]
RETURN 1
ELSE
SRC.SYSMESSAGE You are not a druid.
RETURN 1

[ITEMDEF i_druid_wolfform]
DEFNAME=i_druid_wolfform
ID=i_handr_1
TYPE=t_eq_script
NAME=Druid WolfForm

ON=@CREATE
	TIMER 1
	TAG.PREVCHAR 0
	ATTR=attr_decay

ON=@EQUIP
	TIMER 1

ON=@TIMER
	IF (<CONT.MANA><=5)
	CONT.SYSMESSAGE You are no longer a wolf
	CONT.BODY <EVAL <TAG.PREVCHAR>>
	CONT.FIX
	REMOVE
	RETURN 1
	ELSE
	CONT.SYSMESSAGE You feel the spirit of wolf enter you.
	IF !((<CONT.HITPOINTS>>=(<CONT.STR>))
		CONT.HITPOINTS=<CONT.HITPOINTS> + 10
		CONT.MANA=<CONT.MANA> + (-10)
	TIMER 5
	RETURN 1
	ELSE
	CONT.MANA=<CONT.MANA> + (-10)
	TIMER 5
	RETURN 1

