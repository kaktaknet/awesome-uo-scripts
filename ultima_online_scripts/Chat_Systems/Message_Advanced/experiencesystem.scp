	TIMER 1
	ATTR=attr_decay

ON=@EQUIP
	TIMER 10

ON=@DCLICK
	TARGET
	RETURN 1

ON=@TARGON_ITEM
	CONT.SYSMESSAGE You cannot target an item.
	TIMER 5
	RETURN 1

