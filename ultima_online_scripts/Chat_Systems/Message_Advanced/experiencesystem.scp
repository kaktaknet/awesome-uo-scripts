ON=@Click
	SRC.CHECKSTATS
	IF (<EVAL <TAG.MACROER>>=1)
		MESSAGE [Caught Macroing]
		RETURN 0
	ELSE
	IF (<EVAL <TAG.NONPVP>>=1)
		MESSAGE [<TAG.CLASS>]
		MESSAGE [nopvp]
		RETURN 0
	ELSE
	IF ((<EVAL <TAG.TITLED>><=5) && <EVAL <TAG.TIER>><1))
		MESSAGE [<TAG.CLASS>]
		MESSAGE [Newbie: DNA]
		SRC.SYSMESSAGE DNA: Do not attack!
		RETURN 0
	ELSE
	IF (<EVAL <TAG.EXPLOITER>>=1)
		MESSAGE [Caught Exploiting]
		RETURN 0
	ELSE
	IF (<FLAGS>&statf_criminal)
		MESSAGE [<TAG.CLASS>]
		MESSAGE [V<EVAL <TAG.VOR>>, Tier <EVAL <TAG.TIER>>, Level <EVAL <TAG.TITLED>>]
		RETURN 0
	ELSE
	IF (<KILLS>>4)
		MESSAGE [<TAG.CLASS>]
		MESSAGE [V<EVAL <TAG.VOR>>, Tier <EVAL <TAG.TIER>>, Level <EVAL <TAG.TITLED>>]
		RETURN 0
	ELSE
	IF (<BODY>=c_man) || (<BODY>=c_woman)
		MESSAGE [<TAG.CLASS>]
		MESSAGE [V<EVAL <TAG.VOR>>, Tier <EVAL <TAG.TIER>>, Level <EVAL <TAG.TITLED>>]
		RETURN 0
	ELSE
		TAG.SPEECHCOLOR <SPEECHCOLOR>
		SPEECHCOLOR 00ad
		SPEAK <NAME>
		SPEECHCOLOR <TAG.SPEECHCOLOR>
		RETURN 1
	ENDIF
	RETURN 0
	ENDIF
	RETURN 0

