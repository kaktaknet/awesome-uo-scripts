DESIRES=e_horses,c_man,c_woman
AVERSIONS=e_carnivores3

CATEGORY=Monsters
SUBSECTION=LBR
DESCRIPTION=Kirin


ON=@Create
NPC=brain_animal
FAME={100 3000}
KARMA={-5000 -6999}
STR={550 650}
INT={200 240}
DEX={90 110}

PARRYING={60.0 95.0}
