//////////////////////////////////////////////////////////////////////////////////////////////
//	RIDE FUNCTION									    //
// add this to ride ON=@dclick f_ride i_mt_<ride_item>					    //
// this function is not by me, it was Furios or someones at sphereserver's italy forums     //
//////////////////////////////////////////////////////////////////////////////////////////////


[FUNCTION f_ride]
IF (<SRC.FLAGS>&statf_freeze) || (<DISTANCE>>3) && !(<SRC.ISGM>)
SRC.SYSMESSAGE You can't reach the creature.
ELSEIF !(<SRC.BODY>==c_man) && !(<SRC.BODY>==c_woman) && !(<SRC.BODY>==c_man_gm) 
SRC.SYSMESSAGE You are not physically capable of riding a horse.
ELSEIF !(<ISMYPET>)
SRC.SYSMESSAGE You dont own that horse.
ELSE
VAR.OLDACT=<SRC.ACT.UID>
SRC.ACT=<SRC.FINDLAYER.25.MORE2>
SRC.FINDLAYER.25.REMOVE //dismount
SRC.ACT.P=<SRC.P>
SRC.ACT.DIR=<SRC.DIR>
SRC.ACT.FLAGS=<SRC.ACT.FLAGS>&~statf_ridden
SRC.ACT.UPDATE
SRC.NEWITEM=<ARGS>
SRC.ACT.NAME=<NAME>
SRC.ACT.COLOR=<COLOR>
SRC.ACT.LINK=<SRC.UID>
SRC.ACT.MORE2=<UID>
SRC.ACT.MORE=<BASEID>
SRC.ACT.TIMER=10
SRC.ACT.CONT=<SRC.UID>
ACTARG1=<SRC.ACT.UID>|030000000
ACTION=112
DISCONNECT
FLAGS=<FLAGS>|statf_ridden
SRC.ACT=<VAR.OLDACT>
VAR.OLDACT=
ENDIF
RETURN 1


