[function f_exp_gains]
if (<eval <isitem>> == 1)||(<eval <npc>> > 0 )
return 1
endif
sayu=Exp +<eval <var.expgain>>
tag.exp=<eval <tag.exp>>+<eval <var.expgain>>
tag.nextexp=<eval <tag.nextexp>>+-<eval <var.expgain>>
tag.totalexp=<eval <tag.totalexp>>+<eval <var.expgain>>
if (<eval <tag.nextexp>> < 1 )
tag.statpoints=<eval <tag.statpoints>>+4
tag.nextexp=<eval nexttolevel>
sfx=<qval (<eval <tag.spellaquired>>==5)? snd_spell_cunning: 0>
hits=<str>
mana=<int>
stamina=<dex>
if (<eval <tag.level>> < 10 )
emote=level up
tag.level=<eval <tag.level>>+1
endif
f_get_spell_levels
return 1
endif

[function f_get_spell_levels]
if (<eval <tag.level>> = 2 )
f_aquire_spell
return 1
elseif (<eval <tag.level>> = 4 )
f_aquire_spell
return 1
elseif (<eval <tag.level>> = 6 )
f_aquire_spell
return 1
elseif (<eval <tag.level>> = 8 )
f_aquire_spell
return 1
elseif (<eval <tag.level>> = 10 )
f_aquire_spell
return 1
endif

[events e_exp_gains]
on=@death
var.expgain=<eval <str>/8>
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
findid.i_memory.f_exp_gain_npc
return 0

[function f_exp_gain_npc]
link.f_exp_gains
remove
return 0

[events e_class_system]
on=@click
message=[<tag.magicclass>]
return 0

on=@hit
if (<src.npc>)&&(<eval <src.isevent.e_exp_gains>> = 0 )&&!(<src.flags> & statf_conjured )
src.events=+e_exp_gains
return 0
