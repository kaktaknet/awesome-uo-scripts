[ITEMDEF i_halfskill_pot] //Half Skill Pot
DEFNAME=i_halfskill_pot
ID=i_bottle
NAME=Half Skill Pot

ON=@create
COLOR=022
return 0

ON=@dclick
src.sysmessage You should go to the skill pot stone to use this!
Return 1

[ITEMDEF i_skill_pot] //Full Skill Pot
DEFNAME=i_skill_pot
ID=I_bottle
Name=Full Skill Pot

On=@create
Color=02
return 0

On=@dclick
src.sysmessage You should go to the skill pot stone to use this!
return 1

[ITEMDEF i_skillpot_stone] //Skill Pot Stone
DEFNAME=I_skillpot_stone
ID=I_gravestone_8
Name Skill Pot Stone

ON=@CREATE
Color 0482
return 0

ON=@DCLICK
DIALOG d_skillpot
Return 1

