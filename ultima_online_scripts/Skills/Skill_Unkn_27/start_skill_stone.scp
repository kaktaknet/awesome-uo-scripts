CATEGORY=Maul
SUBSECTION=Stones
DESCRIPTION=Skill Stone

ON=@CREATE
COLOR=00022
ATTR=8010
NAME=Skill Stone [double click]

ON=@DCLICK
SRC.SFX 555
DIALOG d_x_skills_stone1
RETURN 1


