///////////////////////////////////////////////Timer////////////////////////////////////////////////////////

[itemdef i_reward_timer_2]
defname=i_reward_timer_2
name=Reward Timer
id=i_memory
type=t_eq_script

on=@create
attr=attr_invis|attr_move_never

on=@equip
timer=60*30

on=@timer
topbj.sysmessage @88 you may now use the prize book again.
remove
return 1

