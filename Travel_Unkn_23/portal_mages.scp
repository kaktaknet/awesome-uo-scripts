[CHARDEF c_male_portal_mage] 
NAME=#NAMES_HUMANMALE the Realm Portal Mage
ID=C_MAN
CAN=MT_WALK
DESIRES=i_gold,e_notoriety 
AVERSIONS=t_TRAP,t_eerie_stuff 

CATEGORY=Custom NPC's
SUBSECTION=Towns Folk
DESCRIPTION=Male Portal Mage

TSPEECH=spk_human_prime 
TSPEECH=jobdope
TSPEECH=spk_needs 
TSPEECH=spk_rehello 
TSPEECH=spk_human_default 

