[ITEMDEF i_travel_stone]
ID=i_gravestone // Change this to the one you like(изменить на тот, который нравиться)
NAME=Travel Stone
WEIGHT=1000

ON=@CREATE
ATTR=08010

ON=@DCLICK
IF <SRC.FLAGS>&statf_war
SRC.SYSMESSAGE Cant use the travel stone while fighting.
return 1
ELIF <DISTANCE>>2
SRC.SYSMESSAGE You are too far away.
return 1
ELSE
IF <SRC.ACCOUNT.TOTALCONNECTTIME><50000
	SRC.DIALOG travelmenu_10
	return 1
ELIF <SRC.ACCOUNT.TOTALCONNECTTIME><100000
	SRC.DIALOG travelmenu_50
	return 1
ELIF <SRC.ACCOUNT.TOTALCONNECTTIME><150000
	SRC.DIALOG travelmenu_100
ELIF <SRC.ACCOUNT.TOTALCONNECTTIME>>149999
	SRC.DIALOG travelmenu_150
	return 1
ENDIF
ENDIF

