[function init_scroll]
//This function sets the starting variables of the slider
src.tag.maxscroll=<eval <src.act.rescount>-1> //The Maximum pixels that the slider can move
src.tag.scrollspace=21 //The starting position of the slider
src.tag.xloc=370 //This is the x position on the x,y plot
src.tag.yloc=5 //This is the y position on the x,y plot
src.tag.scrollslot 0

[function barclick]
if (<src.act.rescount><2)
src.tag.scrollspace=21
error
elif <src.tag.scrolllines><279
src.tag.totalclicks=<eval (31*9)/<src.tag.scrolllines>>
src.tag.scrollclicked=<eval ((21+(31*9))-0<src.tag.scrollspace>)/0<src.tag.totalclicks>>
if 0<src.tag.scrollclicked>><src.act.rescount>
var.scrollmove=<eval 0<src.tag.scrollclicked>-<src.act.rescount>)>
src.tag.totalclicks=<eval 0<src.tag.totalclicks>-(0<src.tag.totalclicks>*0<var.scrollmove>)>
src.tag.scrollclicked=<eval 0<src.tag.scrollclicked>-0<var.scrollmove>>
endif
src.tag.scrollslot=<eval <src.act.rescount>-0<src.tag.scrollclicked>+1>
endif
if 0<src.tag.scrollslot><0
src.tag.scrollslot=0
endif

[function scrollup]
if (0<src.tag.scrollspace>=21)|(<src.act.rescount><2)
error
elif 0<src.tag.scrollspace>>21
if <src.tag.scrolllines><279
src.tag.totalclicks=<eval (31*9)/<src.tag.scrolllines>>
src.tag.scrollclicked=<eval ((21+(31*9))-0<src.tag.scrollspace>)/0<src.tag.totalclicks>>
if 0<src.tag.scrollclicked>><src.act.rescount>
var.scrollmove=<eval 0<src.tag.scrollclicked>-<src.act.rescount>)>
src.tag.totalclicks=<eval 0<src.tag.totalclicks>-(0<src.tag.totalclicks>*0<var.scrollmove>)>
src.tag.scrollclicked=<eval 0<src.tag.scrollclicked>-0<var.scrollmove>>
endif
src.tag.scrollspace=<eval 0<src.tag.scrollspace>-0<src.tag.totalclicks>>
if 0<src.tag.scrollclicked>>(<src.tag.scrolllines>-2)
src.tag.scrollspace=21
endif
src.tag.scrollslot=<eval <src.act.rescount>-0<src.tag.scrollclicked>+2>
else
src.tag.totalclicks=<eval <src.tag.scrolllines>/(31*9)>
endif
endif
if 0<src.tag.scrollslot><0
src.tag.scrollslot=0
endif

[function scrolldown]
if (0<src.tag.scrollspace>=<eval 21+(31*9)>)|(<src.act.rescount><2)
error
elif 0<src.tag.scrollspace><<eval 21+(31*9)>
if <src.tag.scrolllines><(31*9)
if <src.tag.scrolllines>>19
src.tag.totalclicks=<eval ((31*9)/<src.tag.scrolllines>)>
else
src.tag.totalclicks=<eval (31*9)/<src.tag.scrolllines>>
endif
src.tag.scrollclicked=<eval ((21+(31*9))-0<src.tag.scrollspace>)/0<src.tag.totalclicks>>
if 0<src.tag.scrollclicked>><src.act.rescount>
var.scrollmove=<eval 0<src.tag.scrollclicked>-<src.act.rescount>)>
src.tag.totalclicks=<eval 0<src.tag.totalclicks>+(0<src.tag.totalclicks>*0<var.scrollmove>)>
src.tag.scrollclicked=<eval 0<src.tag.scrollclicked>-0<var.scrollmove>>
endif
src.tag.scrollslot=<eval <src.act.rescount>-0<src.tag.scrollclicked>>
src.tag.scrollspace=<eval 0<src.tag.scrollspace>+0<src.tag.totalclicks>>
if 0<src.tag.scrollclicked><2
src.tag.scrollspace=<eval 21+(31*9)>
endif
else
src.tag.totalclicks=<eval <src.tag.scrolllines>/(31*9)>
endif
if 0<src.tag.scrollslot><1
scrolldown
endif
endif

