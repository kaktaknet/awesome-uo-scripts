		//TIMERD=0.1
		//RETURN 1
	//ELSE
	ENDFOR
	LINK.SAY Done.
	MOREY=4
	LINK.FINDID.I_ARCHITECT.TAG.CURRENTP=<LINK.FINDID.I_ARCHITECT.TAG.STARTP>
	TIMERD=0.1
	RETURN 1
