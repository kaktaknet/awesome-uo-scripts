		LINK.NEWITEM I_MEMORY
		LINK.ACT.MOREP=<LINK.FINDID.I_ARCHITECT.TAG.CURRENTP>
		LINK.FINDID.I_ARCHITECT.TAG.CURRENTX=<LINK.ACT.MOREX>
