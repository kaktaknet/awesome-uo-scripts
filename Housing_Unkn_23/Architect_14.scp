		LINK.NEWITEM <LINK.FINDID.I_ARCHITECT.TAG.NS_WALL>
		LINK.ACT.P=<LINK.FINDID.I_ARCHITECT.TAG.CURRENTP>
		LINK.ACT.MOVE N 1
		LINK.FINDID.I_ARCHITECT.TAG.CURRENTP=<LINK.ACT.P>
		//WEST WALL
		LINK.NEWITEM <LINK.FINDID.I_ARCHITECT.TAG.NS_WALL>
		LINK.ACT.P=<LINK.FINDID.I_ARCHITECT.TAG.CURRENTP>
		LINK.ACT.MOVE W <LINK.FINDID.I_ARCHITECT.TAG.WIDTH>
		//ADJUST X,Y,Z
