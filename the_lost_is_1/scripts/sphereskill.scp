[Function black_gain]			//Skillgain
if (!(RAND(2)) && (<blacksmithing> < 300))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(3)) && (<blacksmithing> >= 300) && (<blacksmithing> < 400))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(4)) && (<blacksmithing> >= 400) && (<blacksmithing> < 500))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(10)) && (<blacksmithing> >= 500) && (<blacksmithing> < 600))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(15)) && (<blacksmithing> >= 600) && (<blacksmithing> < 700))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(20)) && (<blacksmithing> >= 700) && (<blacksmithing> < 800))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(40)) && (<blacksmithing> >= 800) && (<blacksmithing> < 900))
blacksmithing=<blacksmithing>+1
elseif (!(RAND(80)) && (<blacksmithing> >= 900) && (<blacksmithing> < 1000))
blacksmithing=<blacksmithing>+1
elseif (<blacksmithing> >= 1000)
blacksmithing=<blacksmithing>
endif

[Function arms_gain]			//Arms Lore Skillgain
if (!(RAND(2)) && (<armslore> < 300))
armslore=<armslore>+2
elseif (!(RAND(3)) && (<armslore> >= 300) && (<armslore> < 400))
armslore=<armslore>+1
elseif (!(RAND(4)) && (<armslore> >= 400) && (<armslore> < 500))
armslore=<armslore>+1
elseif (!(RAND(6)) && (<armslore> >= 500) && (<armslore> < 600))
armslore=<armslore>+1
elseif (!(RAND(15)) && (<armslore> >= 600) && (<armslore> < 700))
armslore=<armslore>+1
elseif (!(RAND(30)) && (<armslore> >= 700) && (<armslore> < 800))
armslore=<armslore>+1
elseif (!(RAND(50)) && (<armslore> >= 800) && (<armslore> < 900))
armslore=<armslore>+1
elseif (!(RAND(100)) && (<armslore> >= 900) && (<armslore> < 1000))
armslore=<armslore>+1
elseif (<armslore> >= 1000)
armslore=<armslore>
endif
