[ITEMDEF i_mem_repair]
DEFNAME=i_mem_repair
ID=i_gold
TYPE=t_eq_script
NAME=NAME_DES_INGOTS		//Reperatur by Aveltarius
//NAME=<SRC.TARG.NAME>		//Ingot
//COLOR=	<SRC.TARG.COLOR>	//Color...

ON=@CREATE
ATTR=attr_invis|attr_decay
//TIMER=20		

ON=@EQUIP
DCLICK

ON=@DCLICK
TARGET="What do you want to repair?"	
RETURN 1

ON=@TARGON_CHAR
SRC.SYSMESSAGE="That's not repairable !"
RETURN 1

ON=@TARGON_GROUND
SRC.SYSMESSAGE="Target a repairable item !"
RETURN 1

ON=@TARGON_ITEM
IF ((<SRC.TARG.CONT>==<SRC.FINDLAYER(layer_pack).UID> ) //|| ((<SRC.TARG.CONT>==0) && (<SRC.TARG.DISTANCE><=3))) 
ELSE 
SRC.MESSAGE="The item have to be in your backpack." 
RETURN 1
ENDIF 

IF (<SRC.TARG.TYPE>==t_armor) || (<SRC.TARG.TYPE>==t_shield) || (<SRC.TARG.TYPE>==t_weapon_mace_smith) || (<SRC.TARG.TYPE>==t_weapon_mace_sharp) || (<SRC.TARG.TYPE>==t_weapon_sword) || (<SRC.TARG.TYPE>==t_weapon_fence) || (<SRC.TARG.TYPE>==t_weapon_mace_staff) || (<SRC.TARG.TYPE>==t_weapon_mace_crook) || (<SRC.TARG.TYPE>==t_weapon_mace_pick) || (<SRC.TARG.TYPE>==t_weapon_axe)
IF (<SRC.TARG.HITPOINTS>>=<SRC.TARG.MOREM>)
SRC.SYSMESSAGE="The item is already in full repair."
RETURN 1
ELSEIF (<SRC.TARG.HITPOINTS> <=1)
SRC.TARG.REMOVE
SRC.ANIM=9
SRC.SOUND=42
SRC.EMOTE="destroy the <SRC.TARG.NAME>"
RETURN 1
ENDIF
IF (<SRC.TARG.COLOR> != <COLOR> )
SRC.SYSMESSAGE="That have not been produced with this ingot!"
RETURN 1
ENDIF
IF ! (<SRC.TARG.HITPOINTS>)
      SRC.SYSMESSAGE="You can't think the way to repair this item !"
      RETURN 1
ENDIF

//Ingot test
VAR.MISSHITS=<EVAL <SRC.TARG.MOREM>-<SRC.TARG.HITPOINTS> >	//Berechnung...
//SRC.MESSAGE="Variable MISSHITS = <VAR.MISSHITS>"

IF (<VAR.MISSHITS> <=15)
IF (<SRC.RESTEST 1 <NAME>>)
SRC.CONSUME 1 <NAME>
SRC.SYSMESSAGE="You consume 1 ingot."
ELSE
SRC.SYSMESSAGE="You need at least 1 ingot to repair this item."
RETURN 1
ENDIF
ENDIF

IF (<VAR.MISSHITS> >=15) && (<VAR.MISSHITS> <30)
IF (<SRC.RESTEST 2 <NAME>>)
SRC.CONSUME 2 <NAME>
SRC.SYSMESSAGE="You consume 2 ingots."
ELSE
SRC.SYSMESSAGE="You need at least 2 ingots to repait this item."
RETURN 1
ENDIF
ENDIF

IF (<VAR.MISSHITS> >=30) && (<VAR.MISSHITS> <50)
IF (<SRC.RESTEST 3 <NAME>>)
SRC.CONSUME 3 <NAME>
SRC.SYSMESSAGE="You consume 3 ingots."
ELSE
SRC.SYSMESSAGE="You need at least 3 ingots to repair this item."
RETURN 1
ENDIF
ENDIF

IF (<VAR.MISSHITS> >=50) && (<VAR.MISSHITS> <=70)
IF (<SRC.RESTEST 4 <NAME>>)
SRC.CONSUME 4 <NAME>
SRC.SYSMESSAGE="You consume 4 ingots."
ELSE
SRC.SYSMESSAGE="You need at least 4 ingots to repair this item."
RETURN 1
ENDIF
ENDIF

IF (<VAR.MISSHITS> >70)
IF (<SRC.RESTEST 5 <NAME> >)
SRC.CONSUME 5 <NAME>
SRC.SYSMESSAGE="You consume 5 ingots."
ELSE
SRC.SYSMESSAGE="You need at least 5 ingots to repair this item."
RETURN 1
ENDIF
ENDIF

