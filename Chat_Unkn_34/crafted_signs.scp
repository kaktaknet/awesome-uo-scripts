[ITEMDEF i_c_sign_brass]
ID=0bd2
NAME=brass sign
FLIP=0

ON=@DCLICK
IF STRMATCH(0<TOPOBJ.ACCOUNT>,0) 
   SRC.SYSMESSAGE="You can't rename this sign unless its in your backpack." 
   RETURN 1 
ELSE 
   DIALOG d_name_sign
   RETURN 1
ENDIF 

