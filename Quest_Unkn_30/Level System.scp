[FUNCTION f_levelup]
//test
IF (<EVAL <TAG.LEVEL>>==100)
return 1
else
//fin test
TAG.LEVEL=<EVAL <TAG.LEVEL>> +1
tag.statpoints=<eval <tag.statpoints>>+3
