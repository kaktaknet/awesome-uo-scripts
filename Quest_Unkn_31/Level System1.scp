[itemdef i_undead_cloak] 
ID=i_cape 
NAME=Cloak of the Netherworld 

ON=@CREATE 
HITPOINTS={500 1000} 
COLOR=04e7 

ON=@EQUIP 
SRC.EVENTS +e_invis_hit 
IF !(<SRC.SECTOR.LIGHT> > 5) 
SRC.MESSAGE=You may only use this under the cover of nightfall 
RETURN 0 
ELSE 
LINK=<SRC.UID> 
SRC.INVIS=1 
HIDENUKE 1 
SRC.MESSAGE=You have hidden yourself in the shadows 
endif 
TIMER=20 
RETURN 0 


on=@unequip 
SRC.EVENTS -e_invis_hit 
SRC.INVIS=0 
HIDENUKE 1 
LINK=04fffffff 
TIMER=-1 
RETURN 1 

ON=@TIMER 
LINK.MESSAGE You feel the cloak's damage over time 
effect=3,i_fx_curse,6,15,1 
sfx snd_SPELL_CURSE 
link.damage {5 15} 
//link.spelleffect s_poison,50.0,1 
TIMER=20 
RETURN 1 //edit, need this or error on console shows up 

on=@gethit 
SRC.INVIS=0 
HIDENUKE 1 
SRC.MESSAGE=You have been revealed 
return 0 


[EVENTS e_invis_hit] 

ON=@HITTRY 
INVIS=0 
HIDENUKE 1 
SRC.MESSAGE=Attacking has revealed you 
RETURN 0 

ON=@ENVIRONCHANGE //if light or weather changes 
IF (<SRC.FLAGS>&02000) && !(<SRC.SECTOR.LIGHT> > 5) //check if player is invis and region is too bright 
SRC.MESSAGE The new light reveals you! 
INVIS=0 
HIDENUKE 1 
ELSEIF (<SRC.FLAGS>|02000) && !(<SRC.SECTOR.LIGHT> > 5) //if they are already revealed and go into light, do nothing 
RETURN 0 
ELSEIF (<SRC.FLAGS>&02000) && (<SRC.SECTOR.LIGHT> > 5) //if they came from dark to dark, and are invis, do nothing 
RETURN 0 
ELSE //if they come from light to dark or were revealed and came to a dark enough area 
SRC.MESSAGE The shadows cover you once again 
INVIS=1 
HIDENUKE 1 
RETURN 0 
ENDIF 
RETURN 1 

