//The "machine" can that stuff gets dropped into.
[ITEMDEF i_garbage_dumpster]
ID=0e77
NAME="Garbage Reward Dumpster"

ON=@CREATE
   COLOR=07cd

ON=@CLICK
   SRC.SYSMESSAGE "<NAME>"
   SRC.SYSMESSAGE "I can be used by double clicking me and selecting something to trash."
   SRC.SYSMESSAGE "You can use the .trash command to see your reward status."
RETURN 1

ON=@DCLICK
   EMPTY
   TARGET "Select an item to trash, you will NOT get it back if you screw up so don't ask."
RETURN 1

ON=@TARGON_CHAR
   SRC.SYSMESSAGE "<SRC.TARG.NAME> wouldn't appriciate that very much...."
   SRC.TARG.SYSMESSAGE "<SRC.NAME> just tried to throw you in the dumpster...."
RETURN 1

ON=@TARGON_GROUND
   SRC.SYSMESSAGE "Sorry, as surprised as I'm sure you are you still can't throw the ground out."
RETURN 1

ON=@TARGON_ITEM
   IF (0<SRC.TARG.LAYER>==021)
      SRC.SYSMESSAGE "You can't trash that."
      RETURN 1
   ENDIF
   IF (<SRC.TARG.ATTR>&attr_newbie)
      SRC.SYSMESSAGE "You can't trash that."
      RETURN 1
   ENDIF
   IF (<SRC.TARG.TYPE>==t_trash_can)
      SRC.SYSMESSAGE "You can't trash that."
      RETURN 1
   ENDIF
   IF (<SRC.TARG.TOPOBJ.UID>!=<SRC.UID>)
      SRC.SYSMESSAGE "The item has to be on you to trash it."
      RETURN 1
   ENDIF
   IF (<EVAL <SRC.TARG.VALUE>>==0)
      SRC.SYSMESSAGE "That item has no worth, or can not be trashed."
      RETURN 1
   ELIF (<SRC.TARG.TYPE>==t_gold)||(<SRC.TARG.BASEID>==i_ingot_platinum)
      SRC.SYSMESSAGE "Gold is not garbage, even if you have TONS of it...."
      RETURN 1
   ENDIF

   //Vars
   SRC.SYSMESSAGE "You will be given <EVAL (<SRC.TARG.VALUE>*<SRC.TARG.AMOUNT>))> points for this."
   //No points yet
   IF (0<SRC.TAG.GREWARDPTS>==0)
      SRC.TAG.GREWARDPTS=(<EVAL <SRC.TARG.VALUE>>*<SRC.TARG.AMOUNT>)
   ELSE
      VAR.X=(<EVAL <SRC.TAG.GREWARDPTS>>+(<EVAL <SRC.TARG.VALUE>>*<SRC.TARG.AMOUNT>))
      SRC.TAG.GREWARDPTS=<VAR.X>
   ENDIF
   VAR.X=<EVAL <SRC.TAG.GREWARDPTS>>
   SRC.SYSMESSAGE "You now have <EVAL <VAR.X>> points collected for garbage rewards."
   SRC.TARG.REMOVE
RETURN 1

