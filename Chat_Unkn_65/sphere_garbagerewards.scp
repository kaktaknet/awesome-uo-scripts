//******************************************************
//Trash command function
[FUNCTION trash]
   SRC.DIALOG d_trash
RETURN 0

[FUNCTION f_point_check]
   VAR.COST=<ARGN>
   VAR.POINTS=<EVAL <SRC.TAG.GREWARDPTS>>

   //They do not have enough points
   IF (<VAR.COST>><VAR.POINTS>)
      SRC.SYSMESSAGE "You don't have enough points."
      SRC.TAG.FAILED=1
      SRC.MENU m_trash
   ELSE
      f_eat_points <ARGN>
   ENDIF
RETURN 0

[FUNCTION f_eat_points]
   SRC.TAG.GREWARDPTS=((<EVAL <SRC.TAG.GREWARDPTS>>) + (-<EVAL <ARGN>>))
   SRC.SYSMESSAGE "You have <EVAL <SRC.TAG.GREWARDPTS>> points left."
RETURN 0

