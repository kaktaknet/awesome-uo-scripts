        	RETURN 1
	ENDIF

//////////////////////////////////////////////////////// 5

	IF (<SRC.TAG.MACRO_WEAPON_ID5_X>==0)
        	SRC.SYSMESSAGE You add the macro in slot 5.
	        SRC.SOUND=79
        	SRC.TAG.MACRO_WEAPON_NAME5=<SRC.TARG.NAME>
	        SRC.TAG.MACRO_WEAPON_ID5_X=<SRC.TARG.id>
        	RETURN 1
	ENDIF

//////////////////////////////////////////////////////// 6

	IF (<SRC.TAG.MACRO_WEAPON_ID6_X>==0)
        	SRC.SYSMESSAGE You add the macro in slot 6.
	        SRC.SOUND=79
        	SRC.TAG.MACRO_WEAPON_NAME6=<SRC.TARG.NAME>
	        SRC.TAG.MACRO_WEAPON_ID6_X=<SRC.TARG.id>
        	RETURN 1
	ENDIF

//////////////////////////////////////////////////////// 7

	IF (<SRC.TAG.MACRO_WEAPON_ID7_X>==0)
        	SRC.SYSMESSAGE You add the macro in slot 7.
	        SRC.SOUND=79
        	SRC.TAG.MACRO_WEAPON_NAME7=<SRC.TARG.NAME>
	        SRC.TAG.MACRO_WEAPON_ID7_X=<SRC.TARG.id>
        	RETURN 1
	ENDIF

//////////////////////////////////////////////////////// 8

	IF (<SRC.TAG.MACRO_WEAPON_ID8_X>==0)
